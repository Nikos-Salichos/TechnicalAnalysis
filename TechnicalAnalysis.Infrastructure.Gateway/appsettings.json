{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft.AspNetCore": "Trace"
    }
  },
  "AllowedHosts": "*",

  "ReverseProxy": {
    "Routes": {
      "analysis-route": {
        "ClusterId": "analysis-cluster",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "analysis-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://host.docker.internal:5000"
          }
        }
      }
    }
  }
}
