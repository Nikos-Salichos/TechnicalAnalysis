{
  "ConnectionStrings": {
    "PostgreSqlTechnicalAnalysisLocalhost": "Host=localhost; Database=TechnicalAnalysis; Username=postgres; Password=admin",
    "PostgreSqlTechnicalAnalysisDockerCompose": "Host=host.docker.internal; Port=5432; Database=TechnicalAnalysis; Username=postgres; Password=admin",
    "RedisLocalhost": "redis:6379",
    "RedisDockerCompose": "redis:6379"
  },

  "OutputFolder": {
    "Path": "/app/BacktestData"
  },

  "RabbitMq": {
    "Hostname": "rabbitmq",
    "Port": "5672",
    "Username": "guest",
    "Password": "guest"
  },

  "BinanceSettings": {
    "ApiKey": "YourKey",
    "SymbolsPairsPath": "https://api.binance.com/api/v3/exchangeInfo",
    "CandlestickPath": "https://data-api.binance.vision/api/v3/klines"
  },

  "AlpacaSettings": {
    "ApiKey": "YourKey",
    "ApiSecret": "YourSecret"
  },

  "DexSettings": {
    "UniswapEndpoint": "https://api.thegraph.com/subgraphs/name/uniswap/uniswap-v3",
    "PancakeswapEndpoint": "https://api.thegraph.com/subgraphs/name/pancakeswap/exchange-v3-bsc"
  },

  "RateLimitOptions": {
    "AutoReplenishment": true,
    "PermitLimit": 1000000,
    "Window": 10,
    "QueueLimit": 0
  },

  "MailSettings": {
    "DisplayName": "Your DisplayName",
    "From": "Your From",
    "Host": "Your Host",
    "Password": "Your password",
    "Port": "Your Port",
    "EmailAddress": "your email address",
    "UseSSL": false,
    "UseStartTls": true
  },

  "MailData": {
    "To": "",
    "Bcc": ""
  },

  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File", "Serilog.Enrichers.Context" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {SourceContext} [{Level}] {Caller} {Message:}{NewLine}{Exception}]"
        }
      },
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://seq:5341" }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId", "WithCaller" ]
  },

  "AllowedHosts": "*"

}
